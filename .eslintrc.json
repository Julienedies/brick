{
  "env": {
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": "off",
    "not-defined": "off",
    "no-undef": "off",
    "no-unused-vars": "off"
  }
}