{
  "name": "@julienedies/brick",
  "version": "0.8.95",
  "description": "一个借鉴angular开发模式的简洁前端开发框架,帮助组织复杂页面的前端代码.",
  "main": "dist/brick.js",
  "files": [
    "package.json",
    "README.md",
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --config ./build/webpack/index.js --watch",
    "pro": "cross-env NODE_ENV=production webpack --config ./build/webpack/index.js",
    "dev:page": "fis server start --type node --port 3001 --root ./ && fis release -r ./page  -f ./build/fis/page.js -d local -w",
    "pro:page": "fis release -r ./page  -f ./build/fis/page.js -d local -D"
  },
  "keywords": [
    "angular",
    "front",
    "framework"
  ],
  "author": "julienedies",
  "license": "ISC",
  "repository": "http://github.com/Julienedies/brick.git",
  "dependencies": {
    "jquery": "^3.3.0",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "fis": "^1.10.5",
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "^1.11.0",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.11.1",
    "eslint-loader": "^2.1.1",
    "express": "^4.16.4",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "filemanager-webpack-plugin": "^2.0.5",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "install": "^0.12.2",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "open": "^0.0.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.1.5",
    "webpack-shell-plugin": "^0.5.0",
    "yog-devtools": "^0.1.1"
  },
  "resolutions": {
    "terser": "3.14.1"
  }
}
